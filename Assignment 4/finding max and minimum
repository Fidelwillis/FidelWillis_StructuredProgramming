#include <stdio.h>

int main() {
    int num[3];
    int max, min;

    for (int i = 0; i < 3; i++) {
        printf("Enter integer elements: \n");
        scanf("%d", &num[i]);  // fixed by adding &
    }

    max = num[0];
    min = num[0];

    for (int i = 1; i < 3; i++) {
        if (num[i] > max) {
            max = num[i];
        }

        if (num[i] < min) {
            min = num[i];
        }
    }

    printf("Maximum element is: %d\n", max);
    printf("Minimum element is: %d\n", min);

    return 0;
}

